{
  "name": "dotup-ts-node-skills-persistence",
  "version": "0.1.0",
  "description": "Persistence for node skills",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./",
    "postpublish": "git tag -a v%npm_package_version% -m \"Release v%npm_package_version%\" && git push origin --tags",
    "commit-release": "git commit -a -m \"Release v%npm_package_version%\" && git push && git tag -a v%npm_package_version% -m \"Release v%npm_package_version%\" && git push origin --tags",
    "release": "npm run clean-build && npm run test && git push && npm run gh-pages && npm publish",
    "clean-dist": "rimraf dist",
    "clean-build": "rimraf dist && tsc",
    "test": "jasmine-ts",
    "docs": "typedoc --mode file --out docs src --exclude \"**/*+(index|.spec|.e2e).ts\"",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "postversion": "git push && git push --tags",
    "npm-publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotupNET/dotup-ts-node-skills-persistence.git"
  },
  "keywords": [
    "typescript",
    "alexa",
    "skill",
    "dynamodb",
    "persistence"
  ],
  "author": "Peter Ullrich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotupNET/dotup-ts-node-skills-persistence/issues"
  },
  "homepage": "https://github.com/dotupNET/dotup-ts-node-skills-persistence#readme",
  "dependencies": {
    "dotup-ts-logger": "^0.0.13",
    "ask-sdk-dynamodb-persistence-adapter": "^2.5.1"
  },
  "peerDependencies": {
    "dotup-ts-node-skills": "^0.3.4",
    "aws-sdk": "^2.430.0",
    "ask-sdk-core": "^2.5.1",
    "ask-sdk-runtime": "^2.4.0",
    "ask-sdk-model": "^1.13.0"
  },
  "devDependencies": {
    "dotup-ts-node-skills": "^0.3.4",
    "aws-sdk": "^2.430.0",
    "ask-sdk-dynamodb-persistence-adapter": "^2.5.1",
    "ask-sdk-core": "^2.5.1",
    "ask-sdk-runtime": "^2.4.0",
    "ask-sdk-model": "^1.13.0",
    "@types/express": "^4.16.1",
    "@types/i18next-sprintf-postprocessor": "^0.0.29",
    "@types/jasmine": "^3.3.12",
    "gh-pages": "^2.0.1",
    "grunt": "^1.0.4",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-newer": "^1.3.0",
    "grunt-nodemon": "^0.4.2",
    "grunt-run": "^0.8.1",
    "grunt-ts": "^6.0.0-beta.22",
    "grunt-tslint": "^5.0.2",
    "jasmine": "^3.3.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-ts": "^0.3.0",
